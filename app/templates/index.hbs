{{#grid-stack-resizable
  options=(hash animate=true cellHeight=110px width=10 float=true disableOneColumnMode=true minWidth=0)
  onChange=(route-action 'change')
}}
{{#each model as |item|}}
  {{#grid-stack-item
     options=(hash x=item.x y=item.y width=item.width height=item.height)
  }}
  {{#if item.isList}}
    {{#list-grid-item}}
    {{/list-grid-item}}
  {{else if item.isMap}}
    {{#map-grid-item}}
    {{/map-grid-item}}
  {{/if}}
  {{/grid-stack-item}}
{{/each}}
{{/grid-stack-resizable}}
